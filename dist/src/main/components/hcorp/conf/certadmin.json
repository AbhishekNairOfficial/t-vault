{
   "name":"certadmin",
   "path": {
     "metadata/*": {
       "policy": "write"
     },
     "sslcerts/*": {
       "capabilities": [
         "create",
         "update",
         "delete",
         "list"
       ]
     },
     "externalcerts/*": {
       "capabilities": [
         "create",
         "update",
         "delete",
         "list"
       ]
     }
   }
}